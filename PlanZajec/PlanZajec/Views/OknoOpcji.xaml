<Window x:Class="PlanZajec.Views.OknoOpcji"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PlanZajec.Views"
        mc:Ignorable="d"
        Title="OknoOpcji" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="textBoxFiltr" HorizontalAlignment="Left" Height="23" Margin="10,96,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="128"/>
        <Button x:Name="buttonFiltr" Content="Szukaj Potoku" HorizontalAlignment="Left" Height="23" Margin="151,96,0,0" VerticalAlignment="Top" Width="120" Click="buttonFiltr_Click"/>
        <Button x:Name="buttonSzukajProwadzacy" Content="Szukaj po prowadzących" HorizontalAlignment="Left" Margin="48,24,0,0" VerticalAlignment="Top" Width="157" Height="42" Click="buttonSzukajProwadzacy_Click"/>
        <TextBox x:Name="textBoxprowadzacy" HorizontalAlignment="Left" Height="23" Margin="60,96,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
