<Window x:Class="PlanZajec.Views.DrukujWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PlanZajec.Views"
        mc:Ignorable="d"
        Title="Drukowanie" Height="300" Width="300">
    <Grid>
        <Label x:Name="WybierzPlanLabel" Content="Wybierz plan do drukowania" HorizontalAlignment="Left" Margin="51,60,0,0" VerticalAlignment="Top" Width="180"/>
        <ComboBox x:Name="PlanyComboBox" HorizontalAlignment="Left" Margin="51,91,0,0" VerticalAlignment="Top" Width="180"/>
        <Button x:Name="DrukujButton" Content="Drukuj" HorizontalAlignment="Left" Margin="51,134,0,0" VerticalAlignment="Top" Width="180" Height="36" Click="DrukujButton_Click"/>

    </Grid>
</Window>
